idget.TextView" package="com.abc.abcnews" content-desc="" checkable="false" checked="false" clickable="true" enabled="true" focusable="true" focused="false" scrollable="false" long-clickable="true" password="false" selected="false" bounds="[37,296][237,330]" /></node><node index="1" text="Ryan pulls 'fundamentally flawed' GOP health care bill" resource-id="com.abc.abcnews:id/story_title" class="android.widget.TextView" package="com.abc.abcnews" content-desc="" checkable="false" checked="false" clickable="true" enabled="true" focusable="true" focused="false" scrollable="false" long-clickable="true" password="false" selected="false" bounds="[37,362][898,407]" /><node index="2" text="By JUSTIN FISHEL  , JORDYN PHELPS  , MARYALICE PARKS   and CECILIA VEGA  " resource-id="com.abc.abcnews:id/story_authors" class="android.widget.TextView" package="com.abc.abcnews" content-desc="" checkable="false" checked="false" clickable="true" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[37,439][1043,503]" /><node index="3" text="" resource-id="com.abc.abcnews:id/frame_for_story_image" class="android.widget.RelativeLayout" package="com.abc.abcnews" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[0,535][1080,1296]"><node index="0" text="" resource-id="com.abc.abcnews:id/video_cta" class="android.widget.LinearLayout" package="com.abc.abcnews" content-desc="" checkable="false" checked="false" clickable="false" enabled="true" focusable="false" focused="false" scrollable="false" long-clickable="false" password="false" selected="false" bounds="[0,535][1080,1296]"><node index="0" text="" resource-id="com.abc.abcnews:id/video_cta_bar" class="android.widget.RelativeLayout" package="com.abc.abcnews" content-desc="" checkable="false" checked="false" clickable